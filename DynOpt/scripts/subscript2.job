# ----------------------------------------------------------------------------
# MPB

f="mpbcorr"
poschgt="none"
noises=(0.00 0.01 0.05)


# ----------------------------------------------------------------------------



for n in "${noises[@]}"
do
	./run_server.sh "$1" "$2" "$3" "$4" "$f" "$poschgt" "$n"&
done

wait
