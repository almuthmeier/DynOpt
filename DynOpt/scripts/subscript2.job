#!/bin/bash

#SBATCH --gres=gpu:1
#SBATCH --ntasks=8
#SBATCH --mem=32G
#SBATCH --time=3-00:00

# input parameters:
#  $1 -> predictor type
#  $2 -> algnameaddition
#  $3 -> dimension

functions=(sphere rastrigin rosenbrock)
poschgt="sinefreq"

for f in "${functions[@]}"
do
	./run_server.sh $1 $2 $3 $f $poschgt &
done

wait
