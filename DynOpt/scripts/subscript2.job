#!/bin/bash

#SBATCH --gres=gpu:1
#SBATCH --ntasks=8
#SBATCH --mem=32G
#SBATCH --time=3-00:00


# input parameters:
#  $1 -> predictor type
#  $2 -> algnameaddition
#  $3 -> whether uncertainties are used
#  $4 -> dimension


# ----------------------------------------------------------------------------
# sphere, rastrigin, rosenbrock

#functions=(sphere rastrigin rosenbrock)
